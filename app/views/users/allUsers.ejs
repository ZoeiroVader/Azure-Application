<div id="wrapper">
	<div class="main-content">
		<div class="col-xs-12">
			<div class="box-content">
				<h4 class="box-title">Todos os Usuarios</h4>
				<!-- /.box-title -->
				<table id="example" class="table table-striped table-bordered display" style="width:100%">
					<tfoot>
						<tr>
							<th>Nome</th>
							<th>Usuario</th>
							<% if( user.Nivel == 'admin') {%>
								<th>Senha</th>
							<% } %>
							<th>Nivel</th>
							<th>Nome da Empresa</th>
						</tr>
					</tfoot>
					<tfoot>
						<tr>
							<th>Nome</th>
							<th>Usuario</th>
							<% if( user.Nivel == 'admin') {%>
								<th>Senha</th>
							<% } %>
							<th>Nivel</th>
							<th>Nome da Empresa</th>
						</tr>
					</tfoot>
					<tbody>
						<% for(u of users){%>
							<tr>
								<td><%= u.Nome %></td>
								<td><%= u.Usuario %></td>
								<% if( user.Nivel == 'admin') {%>
									<td><%= u.Senha %></td>
								<% } %>
								<td><%= u.Nivel %></td>
								<td><%= user.NomeEmp %></td>
							</tr>
						<% } %>
					</tbody>
				</table>
			</div>
			<!-- /.box-content -->
		</div>
		<footer class="footer">
			<ul class="list-inline">
