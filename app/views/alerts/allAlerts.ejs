<div id="wrapper">
	<div class="main-content">
		<div class="col-xs-12">
			<div class="box-content">
				<h4 class="box-title">Todos os alertas</h4>
				<!-- /.box-title -->
				<table id="example" class="table table-striped table-bordered display" style="width:100%">
					<thead>
						<tr>
							<th>Local</th>
							<th>Temperatura</th>
							<th>Umidade</th>
							<th>Data</th>
							<th>Hora</th>
							<th>Código</th>
						</tr>
					</thead>
					<tfoot>
						<tr>
							<th>Local</th>
							<th>Temperatura</th>
							<th>Umidade</th>
							<th>Data</th>
							<th>Hora</th>
							<th>Código</th>
						</tr>
					</tfoot>
					<tbody>
						<% for(alert of alerts){%>
							<tr>
								<td><%= alert.Local %></td>
								<td><%= alert.Temperatura %>°C</td>
								<td><%= alert.Umidade %>%</td>
								<td><%= alert.DataDMA %></td>
								<td><%= alert.Hora %></td>
								<td><%= alert.Codigo %></td>
							</tr>
						<% } %>
					</tbody>
				</table>
			</div>
			<!-- /.box-content -->
		</div>
		<footer class="footer">
			<ul class="list-inline">
				<li>2019 © DataBytes</li>			
			</ul>
		</footer>
	</div>
	<!-- /.main-content -->
</div><!--/#wrapper -->
